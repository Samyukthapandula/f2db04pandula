extends layout

block content
  h1= title
  -let items=results
  div(class="container-fluid")
  div(class="row")
    each dog,index in items
     div(class="col-sm-2" onclick=`buy(${index})`) #{dog.Dog_age}- #{dog.Dog_breed}- #{dog.Dog_speciality}
    script.
      function buy(dogno){
        alert(`You purchased Item ${dogno+1}`)
      }